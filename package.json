{
  "name": "tsdx-threejs-template",
  "CamelName": "TsdxThreejsTemplate",
  "version": "0.1.0",
  "author": "dwd",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/tsdxthreejstemplate.esm.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/tsdx-threejs-template.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsdx-threejs-template.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/three": "^0.128.0",
    "gh-pages": "^3.2.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "parcel": "^2.0.0-beta.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "size-limit": "^4.11.0",
    "three": "^0.128.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9"
  }
}
